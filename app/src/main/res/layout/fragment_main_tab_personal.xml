<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#E5E5E5"
    >

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHeight_percent="0.92"
        app:layout_constraintWidth_percent="1"
        >

        <View
            android:id="@+id/tab_chat_view_search"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintHeight_percent="0.1"
            android:background="#237EE9"
            />
        <EditText
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:hint="Tìm kiếm"
            android:textColor="@color/white"
            android:textSize="22dp"
            app:layout_constraintWidth_percent="0.55"
            app:layout_constraintHeight_percent="0.08"
            app:layout_constraintHorizontal_bias="0.3"
            app:layout_constraintVertical_bias="0.015"
            android:layout_marginLeft="16dp"
            />
        <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:src="@drawable/ic_baseline_search_white_24"
            app:layout_constraintHorizontal_bias="0.05"
            app:layout_constraintVertical_bias="0.03"
            />
        <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:src="@drawable/ic_baseline_settings_white_24"
            app:layout_constraintHorizontal_bias="0.9"
            app:layout_constraintVertical_bias="0.03"
            />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:background="@color/white"
            app:layout_constraintHeight_percent="0.12"
            app:layout_constraintWidth_percent="1"
            app:layout_constraintHorizontal_bias="0"
            app:layout_constraintVertical_bias="0.12"
            >
            <ImageView
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintWidth_percent="0.2"
                app:layout_constraintHeight_percent="1"
                app:layout_constraintHorizontal_bias="0.02"
                android:src="@drawable/ic_baseline_image_orange_24"
                />
            <TextView
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:text="Tên người dùng"
                android:textStyle="bold"
                android:textSize="16dp"
                android:textColor="@color/black"
                app:layout_constraintWidth_percent="0.5"
                app:layout_constraintHeight_percent="0.4"
                app:layout_constraintVertical_bias="0.15"
                app:layout_constraintHorizontal_bias="0.45"
                />
            <TextView
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:text="Xem trang cá nhân"
                android:textSize="13dp"
                android:textColor="@color/black"
                app:layout_constraintWidth_percent="0.6"
                app:layout_constraintHeight_percent="0.4"
                app:layout_constraintVertical_bias="0.9"
                app:layout_constraintHorizontal_bias="0.58"
                />
            <ImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:src="@drawable/ic_baseline_transfer_within_a_station_black_24"
                app:layout_constraintHorizontal_bias="0.9"
                app:layout_constraintVertical_bias="0.5"
                />

        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:background="@color/white"
            app:layout_constraintHeight_percent="0.1"
            app:layout_constraintWidth_percent="1"
            app:layout_constraintHorizontal_bias="0"
            app:layout_constraintVertical_bias="0.26"
            >
            <TextView
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:text="Thông tin cá nhân"
                android:textStyle="bold"
                android:textSize="18dp"
                android:textColor="@color/black"
                app:layout_constraintWidth_percent="0.5"
                app:layout_constraintHeight_percent="0.35"
                app:layout_constraintVertical_bias="0.25"
                app:layout_constraintHorizontal_bias="0.45"
                />
            <TextView
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:text="Hình đại diện, ngày sinh, giới tính ..."
                android:textSize="13dp"
                android:textColor="@color/black"
                app:layout_constraintWidth_percent="0.6"
                app:layout_constraintHeight_percent="0.35"
                app:layout_constraintVertical_bias="0.9"
                app:layout_constraintHorizontal_bias="0.58"
                />
            <ImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:src="@drawable/ic_baseline_person_black_24"
                app:layout_constraintHorizontal_bias="0.08"
                app:layout_constraintVertical_bias="0.5"
                />

        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:background="@color/white"
            app:layout_constraintHeight_percent="0.1"
            app:layout_constraintWidth_percent="1"
            app:layout_constraintVertical_bias="0.38"
            >
            <ImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:src="@drawable/ic_baseline_cloud_queue_black_24"
                app:layout_constraintHorizontal_bias="0.08"
                app:layout_constraintVertical_bias="0.5"
                />
            <TextView
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:text="Cloud của tôi"
                android:textStyle="bold"
                android:textSize="18dp"
                android:textColor="@color/black"
                app:layout_constraintWidth_percent="0.5"
                app:layout_constraintHeight_percent="0.35"
                app:layout_constraintVertical_bias="0.25"
                app:layout_constraintHorizontal_bias="0.45"
                />
            <TextView
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:text="Lưu trữ các tin nhắn"
                android:textSize="13dp"
                android:textColor="@color/black"
                app:layout_constraintWidth_percent="0.6"
                app:layout_constraintHeight_percent="0.35"
                app:layout_constraintVertical_bias="0.9"
                app:layout_constraintHorizontal_bias="0.58"
                />

        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:background="@color/white"
            app:layout_constraintHeight_percent="0.1"
            app:layout_constraintWidth_percent="1"
            app:layout_constraintVertical_bias="0.5">
            <ImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:src="@drawable/ic_baseline_security_black_24"
                app:layout_constraintHorizontal_bias="0.08"
                app:layout_constraintVertical_bias="0.5"
                />
            <TextView
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:text="Tài khoản và bảo mật"
                android:textStyle="bold"
                android:textSize="20dp"
                android:textColor="@color/black"
                app:layout_constraintWidth_percent="0.8"
                app:layout_constraintHeight_percent="0.35"
                app:layout_constraintVertical_bias="0.5"
                app:layout_constraintHorizontal_bias="1"
                />
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:background="@color/white"
            app:layout_constraintHeight_percent="0.1"
            app:layout_constraintWidth_percent="1"
            app:layout_constraintVertical_bias="0.613">
            <ImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:src="@drawable/ic_baseline_lock_person_black_24"
                app:layout_constraintHorizontal_bias="0.08"
                app:layout_constraintVertical_bias="0.5"
                />
            <TextView
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:text="Quyền riêng tư"
                android:textStyle="bold"
                android:textSize="20dp"
                android:textColor="@color/black"
                app:layout_constraintWidth_percent="0.8"
                app:layout_constraintHeight_percent="0.35"
                app:layout_constraintVertical_bias="0.5"
                app:layout_constraintHorizontal_bias="1"
                />
        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>